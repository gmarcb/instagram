.posts-wrapper.row
  .post
    .post-head
      .name
        Marc Benavidez
    .image.center-block
      =image_tag @post.image.url(:medium)
    %p.caption
      =@post.caption
    .text-center.edit-links
      =link_to "Cancel", posts_path
      |
      =link_to "Edit Posts", edit_post_path(@post)